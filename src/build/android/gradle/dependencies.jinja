dependencies {
{% for path in prebuilts %}
    {{ depCompileName }} files("{{ path }}")
{% endfor %}
{% for proj in java_project_deps %}
    {{ depCompileName }} project(":{{ proj }}")
{% endfor %}
{% for proj in android_project_deps %}
    {{ depCompileName }} project(path: ":{{ proj }}", configuration: "debug")
{% endfor %}
}
