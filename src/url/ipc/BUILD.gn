import("//testing/test.gni")

component("url_ipc") {
  sources = [
    "url_ipc_export.h",
    "url_param_traits.cc",
    "url_param_traits.h",
  ]

  defines = [ "URL_IPC_IMPLEMENTATION" ]

  public_deps = [
    "//ipc",
    "//url",
  ]
  deps = [
    "//base",
  ]
}

test("url_ipc_unittests") {
  sources = [
    "url_param_traits_unittest.cc",
  ]

  deps = [
    ":url_ipc",
    "//base",
    "//base/test:run_all_unittests",
    "//ipc:test_support",
    "//testing/gtest",
    "//url:url",
  ]
}
