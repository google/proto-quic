{
  "name": "Memory Inspector",
  "description": "Memory inspector tool for Android",
  "version": "0.1.5",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "system.storage",
    "unlimitedStorage",
    "fileSystem",
    "webview",
    {
      "socket": [
        "tcp-listen:*:*",
        "tcp-connect",
        "resolve-host",
        "udp-bind:*:*",
        "udp-send-to:*:*"
      ]
    },
    "http://127.0.0.1/*"
  ]
}
